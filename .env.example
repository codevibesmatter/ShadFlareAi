VITE_CLERK_PUBLISHABLE_KEY=

# Logging Configuration
# Set logging level: debug, info, warn, error
VITE_LOG_LEVEL=info

# Set focus mode: none, quiet, debug, or specific contexts (websocket, auth, ai, ui, data, voice, artifacts, performance, system)
# Multiple contexts: websocket,ai,voice
VITE_LOG_FOCUS_MODE=quiet

# Enable/disable patterns (glob patterns supported)
# VITE_LOG_PATTERNS=src/server/ai-chat-websocket.ts,src/lib/voice-websocket-manager.ts
# VITE_LOG_DISABLED_PATTERNS=src/server/*,tests/*

# Filter WebSocket heartbeat messages (true/false)
VITE_LOG_FILTER_HEARTBEATS=true

# Examples for different development scenarios:

# Quiet development (only errors)
# VITE_LOG_FOCUS_MODE=quiet

# Silent mode (no logs)
# VITE_LOG_FOCUS_MODE=none

# Focus on WebSocket debugging
# VITE_LOG_FOCUS_MODE=websocket
# VITE_LOG_FILTER_HEARTBEATS=false

# Focus on AI operations
# VITE_LOG_FOCUS_MODE=ai

# Focus on voice features
# VITE_LOG_FOCUS_MODE=voice

# Multiple contexts
# VITE_LOG_FOCUS_MODE=websocket,ai,voice